{
    "name": "vscode-python-docstring",
    "displayName": "vscode-python-docstring",
    "description": "Create docstring for python methods",
    "version": "0.0.3",
    "publisher": "azaugg",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Other"
    ],
    "main": "./out/src/extension",
    "activationEvents": [
      "onCommand:extension.addPyDocstring"
    ],
    "contributes": {
      "commands": [
        {
          "command": "extension.addPyDocstring",
          "title": "Add Py Docstring"
        }
      ],
      "configuration": {
        "title": "PyDoc configs",
        "properties": {
            "pydocs.style": {
                "type": "string",
                "default": "google",
                "enum": [
                    "google",
                    "numpy"
                ],
                "description": "Set the pydoc string type. google inspired or NumPy: default being google pydocs"
            }
        }
      }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "indent-string": "^2.1.0"
      }
}
